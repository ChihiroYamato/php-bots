{
    "name": "anet/chatbot",
    "description": "PHP ChatBots",
    "autoload": {
        "psr-4": {
            "Anet\\App\\": "src/core/",
            "Anet\\Controllers\\": "src/controllers/",
            "Anet\\Views\\": "src/views/"
        },
        "classmap": [],
        "files": [
            "./src/const.php",
            "./src/env_const.php"
        ]
    },
    "authors": [
        {
            "name": "Alexander Mironov",
            "email": "alexan9610@gmail.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^7.0",
        "google/apiclient": "^2.12.1",
        "google/cloud-dialogflow": "^0.25.0",
        "paquettg/php-html-parser": "^3.1"
    },
    "scripts": {
        "pre-autoload-dump": "Google\\Task\\Composer::cleanup"
    },
    "extra": {
        "google/apiclient-services": [
            "YouTube",
            "YouTubeAnalytics",
            "YouTubeReporting"
        ]
    }
}
